{
    "extends": "airbnb",
    "env": {
        "browser": true
    },
    "rules": {
        "func-names": [
            "error",
            "never"
        ],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/react-in-jsx-scope": "off",
        "react/no-array-index-key": "off",
        "jsx-a11y/no-static-element-interactions": "off",
        "import/no-dynamic-require": "off",
        "comma-dangle": "off",
        "global-require": "off",
        "eqeqeq": "off"
    },
    "globals": {
        "vars": true
    }
}
